<Page x:Class="Shadertoy_to_Gamemaker.channel"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Shadertoy_to_Gamemaker"
      xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
mc:Ignorable="d" 
d:DesignHeight="450"
Title="Common" Width="562" Height="412">

    <Grid>
        <ComboBox SelectedIndex="0" Name="combo" Margin="183,11,226,0" VerticalAlignment="Top" Height="38" SelectionChanged="combo_SelectionChanged"/>
        <ui:Button Visibility="Hidden" Name="import" Icon="{ui:SymbolIcon Symbol=ArrowImport24}" Width="123" Content="导入贴图" Margin="46,11,0,0" VerticalAlignment="Top" Height="38" IsEnabled="False" Click="import_Click"/>
        <ui:Image Name="show" Margin="46,54,46,23" BorderBrush="#7FFFFFFF" />
        <ui:TextBox  Margin="351,11,46,364" RenderTransformOrigin="0.5,0.5" Name="name" Visibility="Hidden">
            <ui:TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="0"/>
                    <RotateTransform/>
                    <TranslateTransform X="0"/>
                </TransformGroup>
            </ui:TextBox.RenderTransform>
        </ui:TextBox>

    </Grid>
</Page>
